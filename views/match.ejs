                <% const A3Court = teams.filter(function(item, index) {
                  if (item.court == 'A-3') return true;
                }); %>
                <p><%= A3Court[0].name %></p>
              </td>
              <td class="opponent-name">
                <% const A4Court = teams.filter(function(item, index) {
                  if (item.court == 'A-4') return true;
                }); %>
                <p><%= A4Court[0].name %></p>
              </td>
              <th class="point-title">point</th>
              <th class="score-title">score</th>
            </tr>
            <tr class="result-cells">
              <td class="self-name">
                <% const A1 = teams.filter(function(item, index) {
                  if (item.court == 'A-1') return true;
                }); %>
                <p><%= A1[0].name %></p>
