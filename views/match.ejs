        <div class="MatchContainer -header">
          <h1>Participation_A</h1>
        </div>
        <div class="league-table">
          <table border="2" bordercolor="black" style="border-collapse: collapse;">
            <tr class="top-tr">
              <th class="team-title">team</th>
              <td class="opponent-name">
                <% const A1Court = teams.filter(function(item, index) {
                  if (item.court == 'A-1') return true;
                }); %>
                <p><%= A1Court[0].name %></p>
              </td>
              <td class="opponent-name">
                <% const A2Court = teams.filter(function(item, index) {
                  if (item.court == 'A-2') return true;
                }); %>
                <p><%= A2Court[0].name %></p>
              </td>
