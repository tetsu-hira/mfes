              <th class="TableContent -number">2</th>
              <td class="TableContent -team"><p><%= B3[0].name %></p></td>
              <td class="TableContent -team"><p><%= B4[0].name %></p></td>
              <td class="TableContent -space"></td>
              <th class="TableContent -referee"><p><%= B1[0].name %></p></th>
